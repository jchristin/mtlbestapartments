{
  "name": "fleub",
  "description": "Web app to find apartment in Montreal",
  "private": "true",
  "version": "1.0.0",
  "engines": {
    "node": "4.x",
    "npm": "3.x"
  },
  "scripts": {
    "sass": "node-sass --include-path scss scss/index.scss public/bundle.css",
    "browserify": "browserify react_components/boot.js -o public/bundle.js",
    "uglifyjs": "uglifyjs public/bundle.js -m -o public/bundle.js",
    "cleancss": "cleancss -o public/bundle.css public/bundle.css",
    "build": "npm run browserify && npm run sass",
    "release": "npm run build && npm run uglifyjs && npm run cleancss",
    "test": "mocha --recursive --require should",
    "postinstall": "npm run release",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julbaxter/fleub.git"
  },
  "contributors": [
    "Mathieu Albrespy <mathieu.albrespy@gmail.com>",
    "Julien Christin <jul.christin@gmail.com>"
  ],
  "license": "Proprietary",
  "dependencies": {
    "body-parser": "^1.15.0",
    "bootstrap": "github:twbs/bootstrap#v4-dev",
    "bootstrap-slider": "^6.1.3",
    "browserify": "^13.0.0",
    "clean-css": "^3.4.10",
    "co": "^4.6.0",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "flexslider": "^2.6.0",
    "font-awesome": "^4.5.0",
    "google-maps-infobox": "^1.1.13",
    "jquery": "^2.2.0",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "mongodb": "^2.1.7",
    "node-sass": "^3.4.2",
    "osm-to-graph": "^0.2.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-masonry-component": "^4.0.0",
    "react-router": "^2.0.1",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.8.0",
    "turf-center": "^1.0.3",
    "turf-convex": "^1.0.1",
    "turf-feature": "^1.0.0",
    "turf-featurecollection": "^1.0.1",
    "turf-inside": "^1.1.4",
    "turf-point": "^2.0.1",
    "turf-polygon": "^1.0.3",
    "uglify-js": "^2.4.24"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "should": "^8.2.2"
  }
}
