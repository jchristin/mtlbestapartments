{
  "name": "fleub",
  "description": "Web app to find apartment in Montreal",
  "private": "true",
  "version": "1.0.0",
  "engines": {
    "node": "4.x",
    "npm": "3.x"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "sass": "node-sass --include-path scss scss/index.scss public/bundle.css",
    "browserify": "browserify react_components/boot.js -t envify -o public/bundle.js",
    "uglifyjs": "uglifyjs public/bundle.js -m -o public/bundle.js",
    "cleancss": "cleancss -o public/bundle.css public/bundle.css",
    "build": "npm run lint && npm run browserify && npm run sass",
    "release": "npm run build && npm run uglifyjs && npm run cleancss",
    "test": "mocha --recursive --require should",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/julbaxter/fleub.git"
  },
  "contributors": [
    "Mathieu Albrespy <mathieu.albrespy@gmail.com>",
    "Julien Christin <jul.christin@gmail.com>"
  ],
  "license": "Proprietary",
  "dependencies": {
    "body-parser": "^1.15.1",
    "bootstrap": "github:twbs/bootstrap#v4-dev",
    "bootstrap-slider": "^7.1.0",
    "browserify": "^13.0.1",
    "clean-css": "^3.4.17",
    "co": "^4.6.0",
    "connect-mongo": "^1.2.0",
    "cookie-parser": "^1.4.3",
    "d3": "^4.1.0",
    "envify": "^3.4.0",
    "eslint": "^3.3.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "flexslider": "^2.6.0",
    "font-awesome": "^4.6.3",
    "google-maps-infobox": "^1.1.13",
    "intl": "^1.2.4",
    "jquery": "^2.2.4",
    "keen-js": "^3.4.1",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "mongodb": "^2.1.7",
    "node-sass": "^3.7.0",
    "osm-to-graph": "^0.2.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "query-string": "^4.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.4.0",
    "react-intl": "^2.0.1",
    "react-masonry-component": "^4.0.0",
    "react-router": "^2.0.1",
    "sendgrid": "^3.0.7",
    "serve-favicon": "^2.3.0",
    "superagent": "^2.0.0",
    "turf-center": "^1.0.3",
    "turf-convex": "^1.0.1",
    "turf-feature": "^1.0.0",
    "turf-featurecollection": "^1.0.1",
    "turf-inside": "^1.1.4",
    "turf-point": "^2.0.1",
    "turf-polygon": "^1.0.3",
    "uglify-js": "^2.4.24"
  },
  "devDependencies": {
    "mocha": "^2.5.3",
    "should": "^9.0.2",
    "supertest": "^1.2.0"
  }
}
