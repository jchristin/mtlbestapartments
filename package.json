{
  "name": "fleub",
  "description": "Web app to find apartment in Montreal",
  "version": "1.0.0",
  "engines": {
    "node": ">=0.10.30 <0.11"
  },
  "scripts": {
    "less": "lessc less/index.less > public/bundle.css",
    "browserify": "browserify react_components/boot.js -o public/bundle.js",
    "uglifyjs": "uglifyjs public/bundle.js -m -o public/bundle.js",
    "cleancss": "cleancss -o public/bundle.css public/bundle.css",
    "build": "npm run browserify && npm run less",
    "release": "npm run build && npm run uglifyjs && npm run cleancss",
    "test": "mocha --recursive --require should",
    "postinstall": "npm run release",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://julbaxter@bitbucket.org/julbaxter/flat-finder.git"
  },
  "contributors": [
    "Mathieu Albrespy <mathieu.albrespy@gmail.com>",
    "Julien Christin <jul.christin@gmail.com>",
    "Xavier Koubi <helloxavierk@gmail.com>"
  ],
  "license": "Proprietary",
  "dependencies": {
    "body-parser": "^1.13.3",
    "bootstrap": "^3.3.5",
    "browserify": "^11.1.0",
    "clean-css": "^3.4.1",
    "express": "^4.13.3",
    "font-awesome": "^4.4.0",
    "google-maps-infobox": "^1.1.13",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "less": "^2.5.1",
    "lodash": "^3.10.1",
    "mongodb": "^2.0.42",
    "osm-to-graph": "^0.2.1",
    "react": "^0.13.3",
    "react-lazy-load": "^1.0.4",
    "react-ribbon": "^0.1.0",
    "react-router": "^1.0.0-rc4",
    "reflux": "^0.2.12",
    "serve-favicon": "^2.3.0",
    "supportkit": "^0.2.30",
    "turf-convex": "^1.0.1",
    "turf-featurecollection": "^1.0.1",
    "turf-inside": "^1.1.4",
    "turf-point": "^2.0.1",
    "turf-polygon": "^1.0.3",
    "uglify-js": "^2.4.24"
  },
  "devDependencies": {
    "mocha": "^2.3.2",
    "should": "^7.1.0"
  }
}
